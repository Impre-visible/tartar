{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "**/*.graphql"
    ],
    "watchAssets": true
  },
  "schematics": {
    "custom-schematics:module": {
      "path": "schematics"
    }
  }
}